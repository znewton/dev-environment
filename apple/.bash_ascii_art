#!/bin/bash

GREY='\033[38;5;244m'
BLUE='\033[38;5;45m'
NC='\033[0m'
JOKE=""
if [ -n "$(type -t dadJoke)" ] && [ "$(type -t dadJoke)" = function ]; then
  JOKE="\n$(dadJoke)\n"
fi

clear
dev_logo="
${GREY}           ${BLUE}              ## ${GREY}
${GREY}       ##  ${BLUE}             ##  ${GREY}    ##
${GREY}      ##   ${BLUE}            ##   ${GREY}     ##
${GREY}     ##    ${BLUE}           ##    ${GREY}      ##
${GREY}    ##     ${BLUE}          ##     ${GREY}       ##
${GREY}   ##      ${BLUE}         ##      ${GREY}        ##
${GREY}  ##       ${BLUE}        ##       ${GREY}         ##
${GREY}   ##      ${BLUE}       ##        ${GREY}        ##
${GREY}    ##     ${BLUE}      ##         ${GREY}       ##
${GREY}     ##    ${BLUE}     ##          ${GREY}      ##
${GREY}      ##   ${BLUE}    ##           ${GREY}     ##
${GREY}       ##  ${BLUE}   ##            ${GREY}    ##
${GREY}           ${BLUE}  ##             ${GREY}

${NC}"

zendesk_logo="${GREY}
  ######################    #######################
  ######################    #####################
   ####################     ###################   
     ################       #################
        ##########          ###############
                       #    #############
                     ###    ###########
                   #####    #########
                 #######    #######
               #########    #####
             ###########    ###
           #############    #        
         ###############          ##########
       #################       ################
     ###################     ####################
   #####################    ######################
 #######################    ######################
${JOKE}
${NC}"

echo -en "$zendesk_logo"

